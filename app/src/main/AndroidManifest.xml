<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="garajemotos_Firebase">

<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.GarajeMotos_MySQL">
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.Activity_Mostrar_Cliente"
        android:exported="false" />
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.activity_mostrar_detalles_cliente"
        android:exported="false" />
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.Activity_Mostrar_Detalles_Moto"
        android:exported="false" />
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.Activity_Mostrar_Moto"
        android:exported="false" />
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.Activity_Nueva_Modifica_Moto"
        android:exported="false" />
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.Activity_Nuevo_Modifica_Cliente"
        android:exported="false" />
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.MainActivity"
        android:exported="false" />
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.activity_Nueva_Cuenta"
        android:exported="false"/>
    <activity
        android:name="com.example.garajemotos_Firebase.Activities.activity_login"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>



